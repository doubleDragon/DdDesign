<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DdBarLayout_LayoutParams">

        <attr name="overlapTop" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="DdCollapsingBarLayout">

        <attr name="dd_contentScrim" format="reference"/>
        <attr name="dd_statusBarScrim" format="reference"/>

    </declare-styleable>

    <declare-styleable name="DdCollapsingBarLayout_LayoutParams">

        <attr name="collapseMode" format="enum">
            <enum name="none" value="0"/>
            <enum name="pin" value="1"/>
            <enum name="parallax" value="2"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="DdToolbar_LayoutParams">
        <attr name="dd_change_alpha" format="boolean"/>
        <attr name="dd_vertical_offset" format="boolean"/>
    </declare-styleable>

</resources>